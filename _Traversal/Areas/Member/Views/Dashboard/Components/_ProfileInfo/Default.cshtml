<div class="col-12 col-xl-4">
    <div class="card">
        
        <div class="card-body p-3">
            <h4 class="card-title">Profil Bilgileri</h4>
            <h6 class="card-subtitle text-muted">Profiliniz ile ilgili bilgiler  , güncellemek için <a asp-action="Update" asp-controller="Profile">tıklayınız</a>   </h6>
            <ul class="list-group mt-3">
                <li class="list-group-item border-0 px-0 ps-0 pt-0 text-sm"><strong class="text-dark">Ad Soyad:</strong> @ViewBag.MemberName</li>
                <li class="list-group-item border-0 px-0 ps-0 pt-0  text-sm"><strong class="text-dark">Telefon:</strong> @ViewBag.MemberPhone </li>
                <li class="list-group-item border-0 px-0 ps-0 pt-0  text-sm"><strong class="text-dark">E-Mail:</strong> @ViewBag.MemberMail</li>
                <li class="list-group-item border-0 px-0 ps-0 pt-0  text-sm"><strong class="text-dark">Konum:</strong> @ViewBag.MemberCountry</li>
                <li class="list-group-item border-0 px-0 ps-0 pt-0  pb-0">

                    <strong class="text-dark text-sm">Sosyal Medya:</strong> &nbsp;
                    <a class="btn btn-facebook btn-simple mb-0 ps-1 pe-2 py-0" href="javascript:;">
                        <i class="fab fa-facebook fa-lg"></i>
                    </a>
                    <a class="btn btn-twitter btn-simple mb-0 ps-1 pe-2 py-0" href="javascript:;">
                        <i class="fab fa-twitter fa-lg"></i>
                    </a>
                    <a class="btn btn-instagram btn-simple mb-0 ps-1 pe-2 py-0" href="javascript:;">
                        <i class="fab fa-instagram fa-lg"></i>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</div>
